{
  "database_url_env": "DATABASE_URL",
  "input": {
    "type": "csv",
    "path": "tmp/wp_credentials.csv",
    "encoding": "utf-8"
  },
  "table": "publishing_site_credentials",
  "match_columns": ["publishing_site_id", "wp_username"],
  "batch_size": 500,
  "allow_issues": true,
  "issues_output_jsonl": "tmp/import_issues/publishing_site_credentials_issues.jsonl",
  "column_map": {
    "publishing_site_id": {
      "lookup": {
        "table": "publishing_sites",
        "match_column": "publishing_site_url",
        "return_column": "id",
        "source_column": "site_url",
        "lookup_normalize": "url_host"
      },
      "required": true
    },
    "auth_type": {
      "value": "application_password"
    },
    "wp_username": {
      "source": "wp_username",
      "transform": "trim",
      "required": true
    },
    "wp_app_password": {
      "source": "wp_app_password",
      "transform": "trim",
      "required": true
    },
    "enabled": {
      "value": true
    }
  }
}
